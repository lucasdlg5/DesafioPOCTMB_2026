{
  "openapi": "3.1.1",
  "info": {
    "title": "TMB_REST | v1",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:51990/"
    },
    {
      "url": "https://localhost:44392/"
    }
  ],
  "paths": {
    "/test": {
      "get": {
        "tags": [
          "TMB_REST"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/orders": {
      "get": {
        "tags": [
          "TMB_REST"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/OrderModel"
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "TMB_REST"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OrderModel"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/orders/{id}": {
      "get": {
        "tags": [
          "TMB_REST"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "pattern": "^-?(?:0|[1-9]\\d*)$",
              "type": [
                "integer",
                "string"
              ],
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "OrderModel": {
        "required": [
          "id",
          "cliente",
          "produto",
          "valor",
          "status",
          "data_Criacao"
        ],
        "type": "object",
        "properties": {
          "id": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          },
          "cliente": {
            "type": "string"
          },
          "produto": {
            "type": "string"
          },
          "valor": {
            "pattern": "^-?(?:0|[1-9]\\d*)(?:\\.\\d+)?(?:[eE][+-]?\\d+)?$",
            "type": [
              "number",
              "string"
            ],
            "format": "double"
          },
          "status": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          },
          "data_Criacao": {
            "type": "string"
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "TMB_REST"
    }
  ]
}